{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",



    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eqeqeq": "warn",
        "indent": [
            "warn",
            4
        ]
    }
}